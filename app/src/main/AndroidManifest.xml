<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dinocodeacademy.maingoquizagain">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activites.ComputerLevelActivity"></activity>
        <activity android:name=".activites.CategoryActivity" />
        <activity android:name=".activites.QuizActivity" />
        <activity android:name=".activites.PlayActivity" />
        <activity android:name=".activites.ResultActivity" />
        <activity android:name=".activites.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activites.HistoryLevelActivity" />
    </application>

</manifest>